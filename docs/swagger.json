    "definitions": {
        "model.AvatorResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "model.OperationResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "msg": {
                    "type": "string"
                }
            }
        },
        "model.Profile": {
            "type": "object",
            "required": [
                "id",
                "nickname"
            ],
            "properties": {
                "avatarurl": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "nickname": {
                    "type": "string",
                    "maxLength": 25,
                    "minLength": 2
                }
            }
        },
        "model.ProfileResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "profile": {
                    "$ref": "#/definitions/model.Profile"
                }
            }
        },
        "model.TokenResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "role": {
                    "type": "integer"
                },
                "token": {
                    "type": "string"
                }
            }
        },
        "model.UIDResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "id": {
                    "type": "string"
                }
            }
        },
        "model.User": {
            "type": "object",
            "required": [
                "id",
                "password",
                "username"
            ],
            "properties": {
                "id": {
                    "type": "string"
                },
                "password": {
                    "type": "string",
                    "maxLength": 20,
                    "minLength": 6
                },
                "role": {
                    "type": "integer"
                },
                "username": {
                    "type": "string",
                    "maxLength": 20,
                    "minLength": 3
                }
            }
        },
        "model.UsersResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "users": {
                    "type": "array",
                    "items": {
